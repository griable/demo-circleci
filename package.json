{
  "name": "demo-circleci",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "bundlewatch": "bundlewatch"
  },
  "dependencies": {
    "core-js": "^3.9.1",
    "nuxt": "^2.15.3"
  },
  "devDependencies": {
    "bundlewatch": "^0.3.2"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "dist/*.js",
        "maxSize": "30 kB"
      },
      {
        "path": "dist/+(pages|static)/*.js",
        "maxSize": "30 kB"
      },
      {
        "path": "dist/vendors/app*.js",
        "maxSize": "70 kB"
      }
    ]
  }
}
